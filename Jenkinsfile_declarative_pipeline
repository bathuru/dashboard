pipeline{
 agent any
  stages{  
    stage('Checkout'){
       steps{
        echo "Checkout"
        }
    }

    stage('Build'){
      steps{
        echo "Build"
        }
    }    
    
  stage('Test'){

 parallel{   
    stage('Unit Test'){
      steps{
        echo "Static Code Analysis"
        }
    }
        
    stage('Integration Test'){
      steps{
        echo "Delivery"
        }
    }
    
    }
    }
  }
}
